{
  "name": "@floatingpixels/nuxt-imagetools",
  "version": "0.0.5",
  "description": "Nuxt module for image manipulation and optimization.",
  "repository": "floatingpixels/nuxt-imagetools",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && release-it",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "preRelease": false,
      "draft": false,
      "tokenRef": "GITHUB_TOKEN"
    },
    "npm": {
      "publish": true,
      "publishArgs": [
        "--access",
        "public"
      ]
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "vite-imagetools": "^7.0.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.2",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.7.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.12.13",
    "changelogen": "^0.5.5",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "nuxt": "^3.11.2",
    "playwright-core": "^1.44.1",
    "prettier": "^3.2.5",
    "release-it": "^17.3.0",
    "vitest": "^1.6.0"
  }
}
